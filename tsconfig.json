{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es2020",
    "target": "es2020",
    "jsx": "react-jsx",
    "allowJs": true,
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths":{
      "*":["./src/*"]
    }
  },
   "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
}